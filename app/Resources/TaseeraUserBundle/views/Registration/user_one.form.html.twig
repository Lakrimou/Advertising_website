{% extends "FOSUserBundle::layout.html.twig" %}

{% block fos_user_content %}
    {% trans_default_domain 'FOSUserBundle' %}

    <form action="{{ path('user_one_registration') }}" {{ form_enctype(form) }} method="POST" data-toggle="validator" role="form" >
        {{ form_widget(form) }}
        <div class="form-group text-center">
            <input type="submit" value="{{ 'registration.submit'|trans }}" class="btn btn-primary sharp btn-md btn-style-one" />
        </div>
    </form>
{% endblock fos_user_content %}